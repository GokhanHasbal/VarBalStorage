                                    <div class="mb-3">
                                        <label for="Address" class="form-label">Address</label>
                                        <textarea id="Address" name="Address" class="form-control" rows="5" required>@Model.SingleAdres.FullAddress</textarea>
                                    </div>
                                    <div class="mb-3">
                                        <label for="PostCode" class="form-label">PostCode</label>
                                        <input type="text" id="PostCode" name="PostCode" class="form-control" value="@Model.SingleAdres.PostCode" required>
                                    </div>
                                </div>

                                <div class="col_third" style="text-align: center">
                                    <label for="bk_name" class="bk_req">Booking Time</label>
                                    <br /><br />

                                    <input class="datefield" data-val="true" data-val-required="Date is required"
                                           id="BookTime" name="BookTime" type="date" value="1/11/1989" />
                                </div>

                                <div class="col_third last">

                                    <div class="mb-3">
                                        <label class="form-label">Warehouse</label>
                                        <select class="form-control" name="WarehouseId" id="WarehouseId">
                                            @foreach (var item in Model.Warehouse)
                                            {

                                                <option value="@item.Id">@item.Name</option>
                                            }

                                        </select>
                                    </div>
                                    <div class="mb-3">
                                        <label class="form-label">Size</label>
                                        <select class="form-control" name="Size" id="Size">
                                            <option value="small"> Small : ton/ 5$ </option>
                                            <option value="medium"> Medium : ton/25$ </option>
                                            <option value="large"> Large : ton/50$ </option>
                                            <option value="xlarge"> XLarge : ton/100$</option>
                                        </select>
                                    </div>
                                    <div class="mb-3">
                                        <label for="Note" class="form-label">Note</label>
                                        <textarea id="Note" name="Note" rows="5" class="form-control"></textarea>
                                    </div>
                                    <div class="form-check mb-2 form-check-success">
                                        <input class="form-check-input" type="checkbox" value="true" id="Status" name="Status" checked="">
                                        <label class="form-check-label" for="customckeck1">Status</label>
                                    </div>
                                    <button class="btn btn-success" type="submit">Book</button>

                                </div>


                            </form>

                        </div><!-- .entry-content-wrapper -->
                    </main><!-- .content-area -->


                </div><!-- inner -->
            </div><!-- #primary.pagemid -->

        </div><!-- #main -->
